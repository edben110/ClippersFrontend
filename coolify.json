{
  "name": "Clipers Frontend",
  "description": "Next.js Frontend for Clipers - Video CV Social Network",
  "type": "dockerfile",
  "dockerfile": "Dockerfile",
  "port": 3000,
  "healthcheck": {
    "enabled": true,
    "path": "/",
    "interval": 30,
    "timeout": 10,
    "retries": 3,
    "start_period": 40
  },
  "build_args": {},
  "resources": {
    "memory": "512Mi",
    "cpu": "0.5",
    "memory_reservation": "256Mi"
  }
}
